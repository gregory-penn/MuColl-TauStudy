                        <LowEnergyCut>1000000000000000000000000.0</LowEnergyCut>
                        <MaxTrackClusterDistance>0.</MaxTrackClusterDistance>
                        <MaxSearchLayer>0</MaxSearchLayer>
                        <ParallelDistanceCut>0</ParallelDistanceCut>
                        <MinTrackClusterCosAngle>1000</MinTrackClusterCosAngle>
